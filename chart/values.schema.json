{
    "title": "Chart Values",
    "type": "object",
    "properties": {
        "nameOverride": {
            "type": "string",
            "description": "",
            "default": ""
        },
        "fullnameOverride": {
            "type": "string",
            "description": "",
            "default": ""
        },
        "replicaCount": {
            "type": "number",
            "description": "This will set the replicaset count ",
            "default": 1
        },
        "image": {
            "type": "object",
            "properties": {
                "repository": {
                    "type": "string",
                    "description": "Image name",
                    "default": "REPOSITORY_NAME/<image name>"
                },
                "tag": {
                    "type": "string",
                    "description": "Overrides the image tag whose default is the chart appVersion.",
                    "default": ""
                },
                "pullPolicy": {
                    "type": "string",
                    "description": "Sets the pull policy for images.",
                    "default": "IfNotPresent"
                }
            }
        },
        "imagePullSecrets": {
            "type": "array",
            "description": "Specify docker-registry secret names as an array",
            "default": [],
            "items": {}
        },
        "volumes": {
            "type": "array",
            "description": "Additional volumes on the output Deployment definition.",
            "default": "[]",
            "items": {
                "type": "string"
            }
        },
        "volumeMounts": {
            "type": "array",
            "description": "Additional volumeMounts on the output Deployment definition.",
            "default": "[]",
            "items": {
                "type": "string"
            }
        },
        "tolerations": {
            "type": "array",
            "description": "",
            "default": "[]",
            "items": {
                "type": "string"
            }
        },
        "podAnnotations": {
            "type": "object",
            "description": "",
            "default": {}
        },
        "podLabels": {
            "type": "object",
            "description": "",
            "default": {}
        },
        "podSecurityContext": {
            "type": "object",
            "description": "",
            "default": {}
        },
        "securityContext": {
            "type": "object",
            "description": "",
            "default": {}
        },
        "serviceAccount": {
            "type": "object",
            "properties": {
                "create": {
                    "type": "boolean",
                    "description": "Enable the creation of a ServiceAccount for Kubewatch pods",
                    "default": false
                },
                "automount": {
                    "type": "boolean",
                    "description": "Automatically mount a ServiceAccount's API credentials?",
                    "default": true
                },
                "annotations": {
                    "type": "object",
                    "description": "Annotations to add to the service account",
                    "default": {}
                },
                "name": {
                    "type": "string",
                    "description": "Name of the created ServiceAccount",
                    "default": ""
                }
            }
        },
        "service": {
            "type": "object",
            "properties": {
                "type": {
                    "type": "string",
                    "description": "The Kubernetes type of the service",
                    "default": "ClusterIP"
                },
                "port": {
                    "type": "number",
                    "description": "The port of the service",
                    "default": 80
                }
            }
        },
        "ingress": {
            "type": "object",
            "properties": {
                "enabled": {
                    "type": "boolean",
                    "description": "",
                    "default": false
                },
                "className": {
                    "type": "string",
                    "description": "",
                    "default": ""
                },
                "annotations": {
                    "type": "object",
                    "description": "",
                    "default": {}
                },
                "hosts": {
                    "type": "array",
                    "description": "",
                    "default": "[]",
                    "items": {
                        "type": "string"
                    }
                },
                "tls": {
                    "type": "array",
                    "description": "",
                    "default": "[]",
                    "items": {
                        "type": "string"
                    }
                }
            }
        },
        "sec": {
            "type": "object",
            "properties": {
                "enabled": {
                    "type": "boolean",
                    "description": "Enable the security module",
                    "default": true
                }
            }
        },
        "redis": {
            "type": "object",
            "properties": {
                "enabled": {
                    "type": "boolean",
                    "description": "Enable the redis subchart",
                    "default": false
                }
            }
        }
    }
}